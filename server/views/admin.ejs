<!DOCTYPE html>
<html>
  <head>
    <title>Admin Page</title>
  </head>
  <body>
    <h1>Admin Page</h1>
    <h3>Users</h3>
    <table border="1">
      <% for(var i=0; i<userProfiles.length; i++) { %>
        <% userProfile = userProfiles[i].attributes %>
        <tr>
          <td>
            <%= userProfile.firstName %>
          </td>
          <td>
            <%= userProfile.lastName %>
          </td>
        </tr>
      <% } %>
    </table>
    <h3>Jobs</h3>
    <table border="1">
      <% for(var i=0; i<jobs.length; i++) { %>
        <% job = jobs[i].attributes %>
        <tr>
          <td>
            <%= job.companyName %>
          </td>
          <td>
            <%= job.contact %>
          </td>
        </tr>
      <% } %>
    </table>

  </body>
</html>
